{
  "name": "restaurantmanager-3.0",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "postinstall": "rimraf ./dist && npm run compile:all",
    "clean": "rimraf ./dist && mkdir dist",
    "lint": "esw  ./test ./routes ./models/ app.js bin/www ",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test mocha --exit",
    "test:report": "cross-env NODE_ENV=test nyc mocha --exit",
    "compile:models": "babel ./models --out-dir dist/models",
    "compile:routes": "babel ./routes --out-dir dist/routes ",
    "compile:controllers": "babel ./controllers --out-dir dist/controllers",
    "compile:auth": " babel ./auth --out-dir dist/auth",
    "compile:db": " babel ./db --out-dir dist/db",
    "compile:app": "babel app.js --out-dir dist/",
    "compile:copy": "ncp ./node_modules/ ./dist/node_modules && ncp ./bin/ ./dist/bin && ncp ./views/ ./dist/views ncp ./controllers/error.js ./dist/controllers/error.js",
    "compile:all": "npm-run-all compile:models compile:routes compile:controllers compile:auth compile:db compile:app compile:copy",
    "start:prod": "cross-env NODE_ENV=dev node dist/bin/www",
    "start:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "prebuild:dev": "npm-run-all test clean",
    "build:dev": "npm run compile",
    "postbuild:dev": "npm run start:prod",
    "build:release": "npm-run-all  test clean compile ",
    "postbuild:release": "rimraf dist/.git dist/test dist/nodemon.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.1.0",
    "chai-http": "^4.3.0",
    "chai-subset": "^1.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cpy-cli": "^2.0.0",
    "debug": "^2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "fa-icons": "^0.1.7",
    "fs-extra": "^8.1.0",
    "googleapis": "^45.0.0",
    "heroku": "^7.35.0",
    "http-errors": "~1.6.3",
    "is-cygwin": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongodb": "^3.3.3",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.7.6",
    "morgan": "~1.9.1",
    "ncp": "^2.0.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-watch": "^6.0.1",
    "mocha": "^6.2.2",
    "mongodb-memory-server": "^6.0.1",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  }
}
