{
  "name": "restaurantmanager-3.0",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postbuild": "npm start",
    "prebuild": "npm-run-all test clean",
    "clean": "rimraf ./dist && mkdir dist",
    "test": "cross-env NODE_ENV=test mocha --timeout 40000 --exit",
    "start": "cross-env NODE_ENV=dev PORT=3001 node dist/bin/www",
    "build": "npm run compile",
    "compile": "babel ./ --out-dir dist/ --ignore ./node_modules,./test --copy-files"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bcrypt": "^3.0.6",
    "chai-subset": "^1.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.15",
    "mongodb": "^3.3.3",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.7.6",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "mocha": "^6.2.2",
    "mongodb-memory-server": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
